bool isPrime(int n)
{
	// Write your code here
	int count=0;
	for(int i=1;i*i<=n;i++)
	{
		int rem = n%i;
		
		if (rem==0)
			{
				count=count+1;
			
                        if ((n / i) != i) 
                                count = count + 1;
                        }
        }
        if (count==2)
			{
      return true;
				
			}
			else 
			return false;
        }
